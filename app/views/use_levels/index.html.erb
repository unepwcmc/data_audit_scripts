<h1>Listing use_levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @use_levels.each do |use_level| %>
      <tr>
        <td><%= use_level.name %></td>
        <td><%= link_to 'Show', use_level %></td>
        <td><%= link_to 'Edit', edit_use_level_path(use_level) %></td>
        <td><%= link_to 'Destroy', use_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Use level', new_use_level_path %>
